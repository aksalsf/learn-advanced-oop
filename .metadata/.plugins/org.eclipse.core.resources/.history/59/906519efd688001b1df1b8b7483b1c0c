package march19;

public class AsynchronousThread extends Thread{
	
	public AsynchronousThread(String name) {
		super(name);
	}
	
	public void run() {
		for(int i=0; i < 10; i++) {
			try {
				Thread.sleep(1000);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
			System.out.println("Thread " + getName() + " at " + i);
		}
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		AsynchronousThread vespa = new AsynchronousThread("Vespa");
		AsynchronousThread onthel = new AsynchronousThread("Onthel");
		
		vespa.start();
		onthel.start();
		
	}

}

