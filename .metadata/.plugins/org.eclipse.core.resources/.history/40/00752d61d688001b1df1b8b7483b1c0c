package march19;

class MyThread extends Thread{
	public MyThread(String name) {
		super(name);
	}
	public static void main(String[] args) {
		Thread I1 = new Thread(new MyThread("I1"));
		Thread I3 = new Thread(new MyThread("I3"));
		I1.start();
		I3.start();
	}
	
}

public static synchronized void print(String name) extends MyThread {
	for (int i = 0; i < 10; i++) {
		try {
			Thread.sleep(1000);
		} catch (InterruptedException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		System.out.println(name + " run " + i + " time");
	}
}
public void run() {
	print(getName());
}
