package march15;

public class ThreadExtend extends Thread{
	
	public ThreadExtend(String name) {
		super(name);
	}
	
	public void run(int loop, int millis) {
		for(int i=0; i < loop; i++) {
			Thread.sleep(millis);
		}
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
